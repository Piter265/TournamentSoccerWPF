<Window x:Class="TournamentSoccer.Views.LaunchTournament"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TournamentSoccer.Views"
        mc:Ignorable="d"
        Title="LaunchTournament" Height="450" Width="800">
    <Grid>
        <TextBlock x:Name="textBlock" HorizontalAlignment="Left" Margin="21,20,0,0" TextWrapping="Wrap" VerticalAlignment="Top"><Run Language="pl-pl" Text="Launch"/><Run Text=" Tournament"/><LineBreak/><Run/></TextBlock>
        <Button x:Name="button" Content="Submit" HorizontalAlignment="Left" Margin="21,98,0,0" VerticalAlignment="Top"/>
        <ComboBox Margin="21,52,630,366">
            <ComboBoxItem>With Rematches</ComboBoxItem>
            <ComboBoxItem>Without Rematches</ComboBoxItem>
        </ComboBox>
    </Grid>
</Window>
